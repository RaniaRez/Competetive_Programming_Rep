class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:

        result=[1]
        i=len(digits)-1
        if digits[-1]!=9:
            digits[-1]+=1
            return digits 
        
        while i>=0:
            if i:
                digits[i]=0
                if digits[i-1]!=9:
                    digits[i-1]+=1
                    return digits

                i-=1
            else:
                digits[i]=0
                return result+digits
